<div class="home-comment-container">
     {{#each this}}
    <div class="home-comment-desc">
        <div class="home-post-container">
            <div id="img-list-home">
                <img  src="{{authorPhoto}}" class="comment-img" alt="">
            </div>
        
            <p>{{authorName}}</p>
            <span>
                "{{description}}"
            </span>
        </div>
    </div>
    {{/each}}
</div>
<div class="lifted-corner-comment">
        <img src="./assets/images/lifted.png" alt="lifted-effect">
    </div>