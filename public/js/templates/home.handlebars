<div class="container">
<div class="home-container row">
        <div id="carousel-item2" class="menu-carousel-container col-md-4 col-sm-12">
                <h3 class="location-title">New in The Menu</h3>
                {{#each this}}
                <div class="menu-item">
                    <div class="menu-list" data-id="{{key}}">
                        <div class="menu-img">
                            <img src="{{img}}" id="img-list" class="img-responsive center-block" alt="">
                        </div>
                        <div class="menu-desc">
                            <div>
                                <span><a href="#/menu/{{key}}" id="menu-profile">{{Title}}</a></span>
                            </div>
                            <div class="menu-desc-hidden">
                                <p>{{Description}}</p>
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>

            <div id="carousel-item1" class=" col-md-4 col-sm-12">
                <h3 class="location-title">Location and hourse of operation</h3>
                <div class="map-holder">
                    <div id="map" class="map-home"></div>
                </div>
                <div class="text-center">
                <p>Viale Sabotino 19, 20135 Milano MI, </p>
                <h4>MON-FRI: <span>6am - 11pm</span> SAT-SUN: <span>7am - 10pm</span></h4>
                </div>
            </div>

            <div class="menu-item col-md-4 col-sm-12">
                <div id="carousel-item3" class="menu-carousel-container">
                    <h3 class="location-title">Customer Testimonials</h3>
                    <div id="home-item3">

                    </div>
                    <div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>